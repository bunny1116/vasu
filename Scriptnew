create  TABLE dbo.SA_GeneralInfo 
(Case_Id int  primary key Identity(1,1) Not Null,
Request_Number int  Null,
Requested_By varchar(100) Not Null,
Master_Account int Not Null,
Sub_Account_Master_Account_Name varchar(25) Not Null,
Sub_Account_Master_Account_TIN int Not Null,
Company_Entity_ID int,
Sub_Account_Number int Not Null,
Name_on_Sub_Account   varchar(25) Not Null,
Agent_Code varchar(25),
BranchAccount varchar(10) Not Null,
Sub_AccountStatus varchar(15),
Sub_AccountClosure Date,
Sub_Account_Agent_Legal_Name varchar(25) Not Null,
Agent_DBA_Trade_Name varchar(25),
Sub_Account_Agent_TIN_EIN int Not Null,
Agent_Address varchar(15) Not Null,
City varchar(15) Not Null,
State varchar(15) Not Null,
ZIP_Code int Not Null,
Country varchar(25) Null,
Agent_Entity_Type varchar(25) Not Null,
Agents_Main_Revenue_Source varchar(25) Not Null,
Purpose_of_Sub_Account varchar(25) Not Null,
Number_of_Agent_Locations int Not Null
);
--

---
create  TABLE dbo.SA_ExpectedActivity  
(ExpectedActivityId int primary key Identity(1,1)  Not Null,
Case_Id int Foreign Key
REFERENCES SA_GeneralInfo(Case_Id),
Monthly_Exp_Dep_Volume varchar(15) Not Null,
Monthly_Exp_Dep_Count varchar(15) Not Null,
Cash_Dep_at_Store_Branch_or_CashVault varchar(15) Not Null,
Monthly_Exp_Cash_Volume varchar(15) Not Null,
Monthly_Exp_Cash_Dep_Count varchar(15) Not Null,
Monthly_Exp_Check_Volume varchar(15) Not Null,
Monthly_Exp_Number_of_Checks_in_Dep varchar(15) Not Null
);
---
create  TABLE dbo.SA_DueDiligence  
(DueDiligenceId int  primary key Identity(1,1) Not Null,
Case_Id int Foreign Key
REFERENCES SA_GeneralInfo(Case_Id),
Due_Diligence_Consultant varchar(25) Not Null,
Initial_Req_Rev_Comp_Date varchar(25) Not Null,
Due_Diligence_Start_Assigned_Date Date Not Null,
Due_Diligence_Completed_Date varchar(25) Not Null,
Due_Diligence_Status int Not Null,
Send_Back_Date varchar(25),
Send_Back_Reason varchar(15),
Return_Date varchar(15),
Approved_in_UW int,
UW_Case_No varchar(25),
Has_Multiple_Subaccounts Date,
OFAC_Last_Screening_Date Date,
Comments varchar(15),
CIG_Level varchar(15),
COPSID int,
COPS_Request_Date Date,
COPS_Submitted_Date Date,
COPS_Completed_Date Date,
CIG_Background_Results varchar(5),
CIG_Background_Results_Comments varchar(4000),
BSAHot_OFAC_PEP_Rev varchar(5),
BSAHot_OFAC_PEP_Comments varchar(4000),
WF_SAR_Review varchar(5) Not Null,
WF_SAR_Review_Comments varchar(4000),
Entity_Verification varchar(5),
Entity_Verification_Comments varchar(4000),
UARFiled bit,
UAR_Reference_No int,
UAR_Date_Filed Date
);
---
create  TABLE dbo.SA_AgentOwner 
(AgentOwnerId int Identity(1,1) primary key,
Case_Id int Foreign Key
REFERENCES SA_GeneralInfo(Case_Id),
Agent_Owners varchar(25),
BusinessLegalName varchar(25),
First_Name varchar(25),
Last_Name varchar(25),
SSN_TIN int,
Street_Address varchar(25),
City varchar(15),
State varchar(15),
ZIP_Code int,
Country varchar(25),
Date_of_Birth Date
);
---

create  TABLE dbo.SA_MATAlerts  
(MATID  int primary key Identity(1,1) Not Null,
Case_Id int Foreign Key
REFERENCES SA_GeneralInfo(Case_Id),
DateAssigned Date,
DateCompleted  Date,
FCAAlertID  varchar(50),
AlertStatus  varchar(25),
Consultant  varchar(25),
AlertType   varchar(25),
AlertAnalysisreview varchar(4000)
);
---
create  TABLE dbo.CDDG_SA_ROLE  
(SA_ROLE_ID int  primary key Identity(1,1) Not Null,
ROLE_NAME varchar(75),
ROLE_DESC varchar(75),
DefaultLandingPage varchar(50),
DefaultLandingPageUrl varchar(100),
CreatedTS Datetime,
CreatedBy varchar(10),
UpdatedTS Datetime,
UpdatedBy varchar(10),
EndDate Datetime null
);
CREATE TABLE [dbo].[CDDGSARoleGroups](
	[RoleGroupID] [int] IDENTITY(1,1) NOT NULL,
	[RoleID] [int] NULL,
	[GroupID] [int] NULL,
	[CreatedTS] [datetime] NULL,
	[CreatedBy] [varchar](10) NULL,
	[UpdatedBy] [varchar](10) NULL,
	[UpdatedTS] [datetime] NULL,
	[EndDate] [datetime] NULL,
 CONSTRAINT [XPKCDDGSARoleGroups] PRIMARY KEY CLUSTERED 
(
	[RoleGroupID] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY],
 CONSTRAINT [XURoleGroup] UNIQUE NONCLUSTERED 
(
	[RoleID] ASC,
	[GroupID] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]

GO
